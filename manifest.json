{
  "manifest_version": 2,
 
  "name": "Duplicate Calls Finder",
  "description": "This extension will look for duplicate calls",
  "version": "1.0",
  "background": {
      "page": "background.html"
   },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Duplicate Calls Finder"
   },
  "offline_enabled": true,
  "permissions": [ "webRequest", "webRequestBlocking", "tabs", "http://*/", "https://*/", "\u003Call_urls>" ]
}